{
    "name": "synergy/solarium-module",
    "description": "Zend Framework 3 integration with solarium solr search client",
    "keywords": [
        "zf3",
        "solr",
        "solarium",
        "search"
    ],
    "license": "MIT",
    "type": "zf3-module",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "doctrine/instantiator": "*",
        "laminas/laminas-component-installer": "*",
        "solarium/solarium": "~3",
        "laminas/laminas-mvc": "*",
        "laminas/laminas-authentication": "*",
        "laminas/laminas-config": "*",
        "laminas/laminas-crypt": "*",
        "laminas/laminas-db": "*",
        "laminas/laminas-di": "*",
        "laminas/laminas-diactoros": "*",
        "laminas/laminas-dom": "*",
        "laminas/laminas-escaper": "*",
        "laminas/laminas-eventmanager": "*",
        "laminas/laminas-file": "*",
        "laminas/laminas-filter": "*",
        "laminas/laminas-form": "*",
        "laminas/laminas-hydrator": "*",
        "laminas/laminas-i18n": "*",
        "laminas/laminas-i18n-resources": "*",
        "laminas/laminas-inputfilter": "*",
        "laminas/laminas-loader": "*",
        "laminas/laminas-log": "*",
        "laminas/laminas-mail": "*",
        "laminas/laminas-math": "*",
        "laminas/laminas-memory": "*",
        "laminas/laminas-mime": "*",
        "laminas/laminas-modulemanager": "*",
        "laminas/laminas-mvc-form": "*",
        "laminas/laminas-mvc-i18n": "*",
        "laminas/laminas-navigation": "*",
        "laminas/laminas-paginator": "*",
        "laminas/laminas-psr7bridge": "^1.8",
        "laminas/laminas-serializer": "*",
        "laminas/laminas-servicemanager": "*",
        "laminas/laminas-session": "*",
        "laminas/laminas-soap": "*",
        "laminas/laminas-tag": "*",
        "laminas/laminas-text": "*",
        "laminas/laminas-uri": "*",
        "laminas/laminas-validator": "*",
        "laminas/laminas-view": "*",
        "laminas/laminas-stdlib": "*",
        "laminas/laminas-dependency-plugin": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "php-coveralls/php-coveralls": "^2.4",
        "phpspec/prophecy": "^1.16",
        "phpspec/prophecy-phpunit": "^2.0"
    },
    "autoload": {
        "psr-0": {
            "SynergySolarium": "src/",
            "SynergySolariumTest": "tests/"
        },
        "classmap": [
            "./tests/SynergySolariumTest/Module.php",
            "./Module.php"
        ]
    },
    "authors": [
        {
            "name": "Pele Odiase",
            "email": "peleodiase@yahoo.co.uk"
        }
    ],
    "config": {
        "allow-plugins": {
            "laminas/laminas-component-installer": true,
            "laminas/laminas-dependency-plugin": true
        }
    }
}
